<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Suiviprojet\AdministrateurBundle\Entity\UserStorie" table="user_storie">
    <id name="idUserStorie" type="integer" column="id_user_storie">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fonctionnalite" type="string" column="fonctionnalite" length="45" precision="0" scale="0" nullable="false"/>
    <field name="role" type="string" column="role" length="45" precision="0" scale="0" nullable="false"/>
    <field name="action" type="text" column="action" precision="0" scale="0" nullable="false"/>
    <field name="but" type="string" column="but" length="45" precision="0" scale="0" nullable="false"/>
    <field name="points" type="integer" column="points" precision="0" scale="0" nullable="false"/>
    <many-to-one field="statutSprintstatutSprint" target-entity="Suiviprojet\AdministrateurBundle\Entity\StatutSprint">
      <join-columns>
        <join-column name="statut_sprint_idstatut_sprint" referenced-column-name="idstatut_sprint" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="statutBacklogstatutBacklog" target-entity="Suiviprojet\AdministrateurBundle\Entity\StatutBacklog">
      <join-columns>
        <join-column name="statut_backlog_idstatut_backlog" referenced-column-name="idstatut_backlog" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="prioritepriorite" target-entity="Suiviprojet\AdministrateurBundle\Entity\Priorite">
      <join-columns>
        <join-column name="priorite_idpriorite" referenced-column-name="idpriorite" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idprojet" target-entity="Suiviprojet\AdministrateurBundle\Entity\Projet">
      <join-columns>
        <join-column name="idProjet" referenced-column-name="idprojet" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="sprintsprint" target-entity="Suiviprojet\AdministrateurBundle\Entity\Sprint" inversed-by="userStorieUserStorie">
      <join-table name="user_storie_has_sprint">
        <join-columns>
          <join-column name="user_storie_id_user_storie" referenced-column-name="id_user_storie"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="sprint_idsprint" referenced-column-name="idsprint" nullable="1"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="developperdevelopper" target-entity="Suiviprojet\AdministrateurBundle\Entity\Developper" inversed-by="userStorieUserStorie">
      <join-table name="user_storie_has_developper">
        <join-columns>
          <join-column name="user_storie_id_user_storie" referenced-column-name="id_user_storie"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="developper_idDevelopper" referenced-column-name="idDevelopper" nullable="1"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
