<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Suiviprojet\AdministrateurBundle\Entity\Sprint" table="sprint">
    <id name="idsprint" type="integer" column="idsprint">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" precision="0" scale="0" nullable="false"/>
    <field name="nbdifficultemax" type="integer" column="nbDifficulteMax" precision="0" scale="0" nullable="false"/>
    <field name="datedebut" type="date" column="dateDebut" precision="0" scale="0" nullable="false"/>
    <field name="datefin" type="date" column="dateFin" precision="0" scale="0" nullable="false"/>
    <many-to-one field="equipeequipe" target-entity="Suiviprojet\AdministrateurBundle\Entity\Equipe">
      <join-columns>
        <join-column name="equipe_idEquipe" referenced-column-name="idEquipe" nullable="1"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="userStorieUserStorie" target-entity="Suiviprojet\AdministrateurBundle\Entity\UserStorie" mapped-by="sprintsprint"/>
  </entity>
</doctrine-mapping>
