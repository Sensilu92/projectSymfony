<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Suiviprojet\AdministrateurBundle\Entity\Projet" table="projet">
    <id name="idprojet" type="integer" column="idprojet">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" precision="0" scale="0" nullable="false"/>
    <field name="dateDebut" type="date" column="date_debut" precision="0" scale="0" nullable="false"/>
    <field name="dateFin" type="date" column="date_fin" precision="0" scale="0" nullable="false"/>
    <field name="description" type="text" column="description" precision="0" scale="0" nullable="true"/>
    <many-to-one field="clientclient" target-entity="Suiviprojet\AdministrateurBundle\Entity\Client">
      <join-columns>
        <join-column name="client_idClient" referenced-column-name="idClient" nullable="1"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
