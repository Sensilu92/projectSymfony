<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Suiviprojet\AdministrateurBundle\Entity\CraHebdomadaire" table="cra_hebdomadaire">
    <id name="idcraHebdomadaire" type="integer" column="idcra_hebdomadaire">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateDebut" type="date" column="date_debut" precision="0" scale="0" nullable="false"/>
    <field name="dateFin" type="date" column="date_fin" precision="0" scale="0" nullable="false"/>
    <field name="commentaire" type="string" column="commentaire" length="45" precision="0" scale="0" nullable="true"/>
    <many-to-many field="developperdevelopper" target-entity="Suiviprojet\AdministrateurBundle\Entity\Developper" inversed-by="craHebdomadairecraHebdomadaire">
      <join-table name="developper_has_cra_hebdomadaire">
        <join-columns>
          <join-column name="cra_hebdomadaire_idcra_hebdomadaire" referenced-column-name="idcra_hebdomadaire"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="developper_idDevelopper" referenced-column-name="idDevelopper" nullable="1"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="craJournaliercraJournalier" target-entity="Suiviprojet\AdministrateurBundle\Entity\CraJournalier" inversed-by="craHebdomadairecraHebdomadaire">
      <join-table name="cra_hebdomadaire_has_cra_journalier">
        <join-columns>
          <join-column name="cra_hebdomadaire_idcra_hebdomadaire" referenced-column-name="idcra_hebdomadaire"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="cra_journalier_idcra_journalier" referenced-column-name="idcra_journalier" nullable="1"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
