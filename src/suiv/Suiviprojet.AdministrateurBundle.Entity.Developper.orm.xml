<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Suiviprojet\AdministrateurBundle\Entity\Developper" table="developper">
    <id name="iddevelopper" type="integer" column="idDevelopper">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nom" type="string" column="nom" length="45" precision="0" scale="0" nullable="false"/>
    <field name="prenom" type="string" column="prenom" length="45" precision="0" scale="0" nullable="false"/>
    <field name="role" type="string" column="role" length="45" precision="0" scale="0" nullable="false"/>
    <field name="login" type="string" column="login" length="45" precision="0" scale="0" nullable="false"/>
    <field name="password" type="string" column="password" length="45" precision="0" scale="0" nullable="false"/>
    <many-to-many field="userStorieUserStorie" target-entity="Suiviprojet\AdministrateurBundle\Entity\UserStorie" mapped-by="developperdevelopper"/>
    <many-to-many field="equipeequipe" target-entity="Suiviprojet\AdministrateurBundle\Entity\Equipe" mapped-by="developperdevelopper"/>
    <many-to-many field="craHebdomadairecraHebdomadaire" target-entity="Suiviprojet\AdministrateurBundle\Entity\CraHebdomadaire" mapped-by="developperdevelopper"/>
  </entity>
</doctrine-mapping>
