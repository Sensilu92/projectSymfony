<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="UserStoriesTechnique" table="user_stories_technique">
    <id name="idUserStoriesTechnique" type="integer" column="id_user_stories_technique">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="role" type="string" column="role" length="45" nullable="true"/>
    <field name="action" type="text" column="action" nullable="true"/>
    <field name="but" type="string" column="but" length="45" nullable="true"/>
    <field name="idpriorite" type="integer" column="idPriorite" nullable="true"/>
    <field name="idstatusus" type="integer" column="idStatusUS" nullable="true"/>
    <many-to-one field="userStorieUserStorie" target-entity="UserStorie">
      <join-columns>
        <join-column name="user_storie_id_user_storie" referenced-column-name="id_user_storie"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idstatut" target-entity="Statut">
      <join-columns>
        <join-column name="idStatut" referenced-column-name="idStatut"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
